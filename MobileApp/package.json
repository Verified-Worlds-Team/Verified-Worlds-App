{
  "name": "game-verifier-backend",
  "version": "1.0.0",
  "description": "Backend API for game skill verification using zkTLS proofs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npm run migrate && npm run seed"
  },
  "keywords": ["game", "verification", "zktls", "blockchain", "xion"],
  "author": "VerifiedwordsTeam",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.1",
    "dotenv": "^16.3.1",
    "sequelize": "^6.32.1",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "axios": "^1.4.0",
    "crypto": "^1.0.1",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "@types/node": "^20.4.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "your-repository-url"
